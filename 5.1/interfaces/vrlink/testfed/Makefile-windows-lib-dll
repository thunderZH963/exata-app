TESTFED_VRLINK_INTERFACE_DIS_DSO_NAME = testfed_vrlink_interface-dis.dll
TESTFED_VRLINK_INTERFACE_HLA13_DSO_NAME = testfed_vrlink_interface-hla13.dll
TESTFED_VRLINK_INTERFACE_HLA1516_DSO_NAME = testfed_vrlink_interface-hla1516.dll


../../bin/$(TESTFED_VRLINK_INTERFACE_DIS_DSO_NAME): force
        copy ..\lib\$(ARCH)-$(COMPILER)\vrlink\$(TESTFED_VRLINK_INTERFACE_DIS_DSO_NAME) ..\bin\$(TESTFED_VRLINK_INTERFACE_DIS_DSO_NAME)

../../bin/$(TESTFED_VRLINK_INTERFACE_HLA13_DSO_NAME): force
        copy ..\lib\$(ARCH)-$(COMPILER)\vrlink\$(TESTFED_VRLINK_INTERFACE_HLA13_DSO_NAME) ..\bin\$(TESTFED_VRLINK_INTERFACE_HLA13_DSO_NAME)

../../bin/$(TESTFED_VRLINK_INTERFACE_HLA1516_DSO_NAME): force
        copy ..\lib\$(ARCH)-$(COMPILER)\vrlink\$(TESTFED_VRLINK_INTERFACE_HLA1516_DSO_NAME) ..\bin\$(TESTFED_VRLINK_INTERFACE_HLA1516_DSO_NAME)


#
# Targets
#
TESTFED_VRLINK_INTERFACE_SHARED_OBJECTS = \
    ../../bin/$(TESTFED_VRLINK_INTERFACE_DIS_DSO_NAME) \
    ../../bin/$(TESTFED_VRLINK_INTERFACE_HLA13_DSO_NAME) \
    ../../bin/$(TESTFED_VRLINK_INTERFACE_HLA1516_DSO_NAME)


ADDON_DEPS      = $(ADDON_DEPS) $(TESTFED_VRLINK_INTERFACE_SHARED_OBJECTS)

clean-vrlink-testfed-all:
    -del ..\bin\$(TESTFED_VRLINK_INTERFACE_DIS_DSO_NAME) > NUL 2>&1
    -del ..\bin\$(TESTFED_VRLINK_INTERFACE_HLA13_DSO_NAME) > NUL 2>&1
    -del ..\bin\$(TESTFED_VRLINK_INTERFACE_HLA1516_DSO_NAME) > NUL 2>&1


ADDONS_CLEAN = $(ADDONS_CLEAN) clean-vrlink-testfed-all

force :
